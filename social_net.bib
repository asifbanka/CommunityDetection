@inproceedings{DHR94,
  author =   {C. Damm and M. Holzer and P. Rossmanith},
  title =    {Expressing Uniformity via Oracles},
  booktitle =    {Developments in Theoretical Computer Science:
                  Proceedings of the International Meeting of Young
                  Computer Scientists},
  year =     1994,
  editor =   {J. Dassow and A. Kelemenova},
  publisher =    {Gordon and Breach Science Publishers},
  web =      {weblinkps{dahoro.ps}},
  abstract =     {We study under what circumstances different
                  uniformity notions for $NC^1$ lead to the same
                  complexity class. Our investigations are based on a
                  characterization of uniformity in terms of oracle
                  access to tally sets which is proved in the paper:
                  $\cal A$-uniform $NC^1$-circuits are of the same
                  computational power as \DLOGTIME-uniform
                  $NC^1$-circuits with oracle access to tally sets in
                  $\cal A$.  This characterization applies not only to
                  classes $\cal A$ such as $\L$ or $\P$ but to all in
                  some sense ``well-behaved'' classes and especially
                  to all standard complexity classes. We present many
                  applications for this characterization: among them
                  upward separations, depth-uniformity trade-offs, and
                  inclusion-completeness results for tally languages.}
}

