@article{BMS05,
  author =   "Byskov, Jesper Makholm and Madsen, Bolette
                  Ammitzb{\o}ll and Skjernaa, Bjarke",
  title =    "On the Number of Maximal Bipartite Subgraps of a
                  Graph",
